footer 
	background: #1A1A1A
	padding: 67px 0 50px 0
	box-shadow: 0px 0px 25px rgba(0, 0, 0, 0.8)

	.container
		// max-width: 100%
		display: flex
		color: #fff
		align-items: flex-start
		justify-content: space-between
		// padding: 0px 40px 0 14px
		@media mqSmallDesktop
			flex-wrap: wrap
			padding: 0px 40px 0 40px
		@media mqBigMobile
			padding: 0 20px

		& > div 
			display flex
			width: 100%
			flex-wrap: wrap
		& + .container
			margin-top: 20px
			// justify-content: space-between
		.email,.phone,.address
			display flex
			align-items: center
			flex-wrap: nowrap
			img 
				margin-right: 5px
		.socials
			justify-content: flex-end
			img 
				margin-left: 5px

	ul 
		margin-top: 0px
		width: 100%
		display: inline-block
		vertical-align: top
		display flex

		li
			margin: 8px 0
			a
				font-weight: 300
				letter-spacing: 0.8px
				font-size: 14px
				&:hover
					text-decoration: underline

